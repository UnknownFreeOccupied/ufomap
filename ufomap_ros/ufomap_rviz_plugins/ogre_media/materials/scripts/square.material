material ufomap_rviz_plugins/Square
{
	technique gp
	{
		pass
		{
      alpha_rejection greater_equal 1
			vertex_program_ref ufomap_rviz_plugins/glsl/pass_pos_color.vert {}
			geometry_program_ref ufomap_rviz_plugins/glsl/billboard.geom {}
			fragment_program_ref ufomap_rviz_plugins/glsl/smooth_square.frag {}
		}
	}

	technique gp_depth
	{
		scheme Depth
		pass
		{
			alpha_rejection greater_equal 1
			vertex_program_ref ufomap_rviz_plugins/glsl/pass_pos_color.vert {}
			geometry_program_ref ufomap_rviz_plugins/glsl/billboard.geom(with_depth) {}
			fragment_program_ref ufomap_rviz_plugins/glsl/depth.frag {}
		}
	}

	technique gp_selection_first_pass
	{
		scheme Pick
		pass
		{
			vertex_program_ref ufomap_rviz_plugins/glsl/pass_pos_color.vert {}
			geometry_program_ref ufomap_rviz_plugins/glsl/billboard.geom {}
			fragment_program_ref ufomap_rviz_plugins/glsl/pick_color.frag {}
		}
	}

	technique gp_selection_second_pass
	{
		scheme Pick1
		pass
		{
			alpha_rejection greater_equal 1
			vertex_program_ref ufomap_rviz_plugins/glsl/pass_pos_color.vert {}
			geometry_program_ref ufomap_rviz_plugins/glsl/billboard.geom {}
			fragment_program_ref ufomap_rviz_plugins/glsl/pass_color.frag {}
		}
	}
}